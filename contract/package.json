{
    "name": "medicalContract",
    "version": "1.2.1",
    "description": "node.js chaincode on Hyperledger-fabric for medical records",
    "main": "index.js",
    "engines": {
        "node": ">=8",
        "npm": ">=5"
    },
    "scripts": {
        "test": "nyc mocha --recursive",
        "start": "fabric-chaincode-node start"
    },
    "engineStrict": true,
    "author": "KB Lee",
    "license": "Apache-2.0",
    "dependencies": {
        "fabric-contract-api": "^1.4.2",
        "fabric-shim": "^1.4.2"
    },
    "devDependencies": {
        "chai": "^4.2.0",
        "eslint": "^6.6.0",
        "mocha": "^10.2.0",
        "nyc": "^14.1.1",
        "sinon": "^7.5.0",
        "sinon-chai": "^3.3.0"
    },
    "nyc": {
        "exclude": [
            "coverage/**",
            "test/**"
        ],
        "reporter": [
            "text-summary",
            "html"
        ],
        "all": true,
        "check-coverage": true,
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
    }
}
